.chrome-tabs-shell.chrome-tabs-dark-theme
  background #1e1e1e

  .chrome-shell-bottom-bar
    background #505050 // TODO
    box-shadow 0 -.05em #161819 // TODO

  .chrome-tabs

    .chrome-tab

      .chrome-tab-background

        > svg

          .chrome-tab-shadow
            stroke #161819 // TODO

          .chrome-tab-background
            fill #373737

      &.chrome-tab-current .chrome-tab-background > svg .chrome-tab-background
          fill #505050

      .chrome-tab-title
        color #fff

      .chrome-tab-close:not(:hover)
        color #cfcfcf
